class Finder{
{
	tab;
}
{
	def {set, t}
	{
		self.tab = t;
	}
	def {find, value}
	{
		index = 0;
		lower = 0;
		upper = self.tab<-{size};
		while(self.tab<-{itemAt, index} != value)
		{
			index = (upper+lower)/2;
			if(value<self.tab<-{itemAt, index})
			{
				upper = index;
			}
			if(self.tab<-{itemAt, index}<value)
			{
				lower = index;
			}
		}
		return index;
	}
}
}

a = [0,1,3,4,4,5,6,18,76,115,654,942];
fin = Finder<-{new};
fin<-{set, a};
stdio<-{print,fin<-{find, 3}};

